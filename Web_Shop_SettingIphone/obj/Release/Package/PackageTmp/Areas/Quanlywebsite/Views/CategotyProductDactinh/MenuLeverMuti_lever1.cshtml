@model IList<Web_Shop_SettingIphone.Models .Entity .GroupMenuSanPham>
@if (Model.Count > 0)
{
    <div class="col-lg-4 col-md-4 col-sm-4">
        <div class="form-group">
            <label class="nguonsanxuat">Cùng thể loại Sản Phẩm</label> <br />
            @foreach (var item in Model)
            {
                <input type="checkbox" name="Ck_Sanphamcungloai" value="@item.Tag" /> @item.Name  <br />
                {
                    Html.RenderAction("MenuLeverMuti_lever2", "CategotyProductDactinh", new { id = item.Id, select = ViewBag.select });
                }
            }

        </div>
    </div>
}