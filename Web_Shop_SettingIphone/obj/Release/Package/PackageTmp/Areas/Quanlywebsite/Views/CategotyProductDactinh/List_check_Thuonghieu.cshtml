@model IList<Web_Shop_SettingIphone.Models.Entity.Thuonghieu>
           @if(Model.Count>0)
           {
              <div class="col-lg-2 col-md-2 col-sm-2">
                 <div class="form-group">
                     <label class="thuonghieu">Thương hiệu</label><br />
                     @foreach (var item in Model)
{
    <input type="checkbox" name="Ck_IDthuonghieu"  value="@item.Tenthuonghieu" /> @item.Tenthuonghieu <br />
                       }

                  </div>
             </div> 
           }