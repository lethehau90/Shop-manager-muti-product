<script type="text/javascript">
    var editor;
    function createEditor(languageCode, id) {
        var editor = CKEDITOR.replace(id, {
            language: languageCode
        });
    }
    $(function () {
        createEditor('vi', 'TxtDetail'); 
        createEditor('vi', 'TxtDetailEn');
        createEditor('vi', 'TxtDacDiemNoiBat');
        createEditor('vi', 'TxtDacDiemNoiBatEn');
    });

</script>

