@model IList<Web_Shop_SettingIphone.Models.Entity.DientuMenuSitemathang>
@if(Model.Count>0)
{
    <ul class="nav nav-third-level">
        @foreach(var item in Model)
        {
            int Countgroup = Convert.ToInt32((Html.Action("Count_category", "DientuMenuSitemathangAdmin", new { Id = item.Id }).ToHtmlString()));
            <li>
                @if (Countgroup > 0)
                {
                    <a href="@Url.Action("Read", "DientuMathangAdmin", new { tag = item.Tag })"><span class="fa arrow"></span><i class="fa fa-check-square"></i> @item.MenuName</a>
                }
                else
                {
                    <a href="@Url.Action("Read", "DientuMathangAdmin", new { tag = item.Tag })"><i class="fa fa-check-square"></i> @item.MenuName</a>
                }
                @{Html.RenderAction("Muti_Group_Menu_2", "MenuGroupproductMuti", new { Id = item.Id });}
            </li>
          
        }
    </ul>
}