<!--starr Back To Top-->
<a id='noop-top' style='display: none; position: fixed;
bottom: 1px; right:1%; cursor:pointer;font:12px arial;'><img src='/Images/Top.png' /></a>
<script type='text/javascript'>
    $(function () {
        $(window).scroll(function () {
            if ($(this).scrollTop() != 0) {
                $("#noop-top").
                fadeIn()
            } else {
                $("#noop-top").fadeOut()
            }
        });
        $("#noop-top").
        click(function () {
            $("body,html").animate({
                scrollTop: 0
            }, 800);
            return false
        })
    });
</script>

<script>
    $("#menu-toggle").click(function (e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
</script>

<script type="text/javascript">
    $(document).ready(function () {
        var flag = true;
        $("#BtSearchMain").click(function () {
            if ($("#SearchMain").val() == "") {
                $.toast({
                    heading: 'Thông báo',
                    text: 'Bạn chưa nhập ký tự tìm kiếm Sản Phẩm.',
                    icon: 'info',
                    position: 'mid-center',
                    stack: false
                })
                flag = false;
            }

            return flag;
        });
    });


</script>
<!--starr End To Top-->
