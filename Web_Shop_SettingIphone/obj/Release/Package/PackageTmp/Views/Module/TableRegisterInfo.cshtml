<div class="bangdangky-1">
    <h6>Sửa chữa Iphone, Ipad, Samsumg  </h6>
    <p>Gởi cho chúng tôi tình trạng máy của bạn</p>

    <div class="clear20"></div>
    <form action="">
        <!-- start thuonghieu -->
        <p>Thương hiệu</p>
        <select name="Selectbangdangky1TH" id="Selectbangdangky1TH">
            <option value="">Chọn thương hiệu</option>
        </select>
        <!-- end thuonghieu -->
        <div class="clear30"></div>
        <!-- start dongmay -->
        <p>Dòng máy</p>
        <span id="Showselect1"></span>
        <!-- end dongmay -->
        <div class="clear30"></div>
        <!-- start số seri -->
        <p>Số seri</p>
        <input type="text" id="Dk1Soseri" placeholder="Điền số seri vào đây">
        <!-- end số seri -->
        <div class="clear30"></div>
        <!-- start hovaten -->
        <p>Họ và tên</p>
        <input type="text" id="Dk1Hovaten" placeholder="Nhập họ và tên">
        <!-- end hovaten -->
        <div class="clear30"></div>
        <!-- start sodienthoai -->
        <p>Số điện thoại </p>
        <input type="text" id="Dk1Sodienthoai" onkeypress="return isNumberKey(event, this);" placeholder="Nhập số điện thoại vào đây">
        <!-- end sodienthoai -->
        <div class="clear30"></div>
        <!-- start noidungsuachua -->
        <p>Nội dung sữa chữa</p>
        <textarea name="" id="Dk1Noidungsuachua" cols="0" rows=""></textarea>
        <!-- end noidungsuachua -->
        <div class="clear30"></div>
        <!-- start hinh anh -->
        <p>File đính kèm</p>
        <input type="file" id="Dk1Images1" placeholder="chọn hình"/>
        <div class="hidden"><input id="Dk1file_name_Images1" /></div>
        <!-- end hinh anh -->
        <div class="clear30"></div>
        <!-- start hinh anh -->
        <p>File đính kèm</p>
        <input type="file" id="Dk1Images2" placeholder="chọn hình"/>
        <div class="hidden"><input id="Dk1file_name_Images2" /></div>
        <!-- end hinh anh -->
        <div class="clear40"></div>
        <button id="createdangky1" type="button"> Gửi thông tin</button>
    </form>
</div>