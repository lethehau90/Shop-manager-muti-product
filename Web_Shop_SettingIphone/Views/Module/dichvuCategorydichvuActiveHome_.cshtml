@model IList<Web_Shop_SettingIphone.Models.Entity.GroupNewsNewsDetail>
@if (Model.Count > 0)
{
    <div class="hinhhienthi col-md-12 col-xs-12 col-sm-12 col-lg-8">
        @foreach (var item in Model)
        {
            <div class="sp col-md-3 col-sm-3">
                <a href="/dich-vu/@item.Cateprolevel3/@item.Tag">
                    <img src="@item.Image" alt="@item.Name">
                </a>
                <p>
                    <a href="/dich-vu/@item.Cateprolevel3/@item.Tag">@item.Name</a>
                </p>
            </div>
        }
    </div>

}