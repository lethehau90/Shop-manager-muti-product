@model IList<Web_Shop_SettingIphone.Models.Entity.Advertise>


<section class="logo col-sm-12 col-xs-12 logo-wrap text-center">

    @if (Model.Count > 0)
    {
        <a href="/">
            <img src="@Model[0].Image" alt="@Model[0].Name"></a>
    }
    else
    {
        <img src="/images/no-image.gif" alt="Chưa có hình">
    }
</section>

<!-- start nav -->
@{Html.RenderAction("nav", "Module");}
<!-- end nav -->
