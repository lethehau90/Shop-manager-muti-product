@model IList<Web_Shop_SettingIphone.Models.Entity.Mausac>
@if (Model.Count > 0)
{
    <div class="col-lg-2 col-md-2 col-sm-2">
        <div class="form-group">
            <label class="mausac">Màu sắc</label><br />
            @foreach (var item in Model)
{
    <input type="checkbox" name="Ck_Mausac"  value="@item.Tenmau" /> @item.Tenmau
                <img class="ShowImgZoom" src="@item.Hinhanh" /> <br />
            }

        </div>
    </div>
}