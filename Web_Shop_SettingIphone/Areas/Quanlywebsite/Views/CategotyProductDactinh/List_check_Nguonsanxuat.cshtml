@model IList<Web_Shop_SettingIphone.Models.Entity.Nuocsanxuat>
@if (Model.Count > 0)
{
    <div class="col-lg-2 col-md-2 col-sm-2">
        <div class="form-group">
            <label class="nguonsanxuat">Nguồn sản xuất</label><br />
            @foreach (var item in Model)
{
    <input type="checkbox" name="Ck_NguonSanPham"  value="@item.TenNuocsanxuat" /> @item.TenNuocsanxuat <br />
            }
        </div>
    </div>
}