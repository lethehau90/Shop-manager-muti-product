@model IList<Web_Shop_SettingIphone.Models.Entity.GroupMenuSanPham>
<div class="row">
    <div class="col-lg-12">
        <div class="form-group">
            <label class="control-label col-lg-12">Danh Mục</label>
            <div class="col-md-3">
                <select id="SelectCategory" name="SelectCategory" class="form-control chosen">
                    <option value="">Chọn danh mục</option>
                    @foreach (var item in Model)
                    {
                        <option value="@item.Id">
                            |-- @item.Name
                        </option>
                        {
                            Html.RenderAction("MenuLeverMuti_product_lever2", "MenuLeverMuti", new { id = item.Id, select = ViewBag.select });
                        }
                    }
                </select>
            </div>
            <div class="col-md-3">
                <label class="checkbox-inline"> Value default </label>
                <input id="CKdisabled" class="checkbox-inline" type="checkbox" checked="checked">
            </div>
        </div>
    </div>
</div>
