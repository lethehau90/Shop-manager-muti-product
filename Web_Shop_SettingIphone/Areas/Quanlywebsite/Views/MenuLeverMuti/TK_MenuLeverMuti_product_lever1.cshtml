@model IList<Web_Shop_SettingIphone.Models.Entity.GroupMenuSanPham>


<select id="select" name="SelectCategory_tk" class="form-control chosen">
    <option value="">Chọn danh mục</option>
    @foreach (var item in Model)
    {
        <option value="@item.Id">
            |-- @item.Name
        </option>
        {
            Html.RenderAction("MenuLeverMuti_product_lever2", "MenuLeverMuti", new { id = item.Id, select = ViewBag.select });
        }
    }
</select>

